    // techdoc:description "Recommended. The headline/title of the article. Bing supports Schema.org Article, NewsArticle, and BlogPosting types."
    // techdoc:identifier "warning";

    schema:image +
    // techdoc:url "https://www.bing.com/webmasters/help/marking-up-your-site-with-structured-data-3a93e731"
